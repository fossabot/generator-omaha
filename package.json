{
  "name": "generator-techtonic",
  "version": "1.3.1",
  "description": "Yeoman generator designed to help you craft maintainable code for the modern web",
  "license": "MIT",
  "main": "generators/app/index.js",
  "repository": "jhwohlgemuth/generator-techtonic",
  "author": {
    "name": "Jason Wohlgemuth",
    "email": "jhwohlgemuth@gmail.com",
    "url": "https://github.com/jhwohlgemuth"
  },
  "engines": {
    "node": "*"
  },
  "files": [
    "generators/app",
    "generators/module",
    "generators/server",
    "generators/env"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "config": "^1.17.1",
    "mkdirp": "^0.5.1",
    "underscore": "^1.8.3",
    "yeoman-generator": "^0.19.0",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.1",
    "mocha": "*",
    "mocha-lcov-reporter": "^1.0.0",
    "mocha-unfunk-reporter": "^0.4.0",
    "sinon": "^1.17.2"
  },
  "scripts": {
    "mocha": "mocha -R mocha-unfunk-reporter",
    "test": "istanbul cover _mocha",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "travis": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "posttravis": "npm run coveralls",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  }
}
